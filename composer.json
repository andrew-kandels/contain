{
    "name": "akandels/contain",
    "description": "Zend Framework 2 library for the passing and handling of rich data entities.",
    "type": "library",
    "keywords": [
        "zf2","entity","hydrator","contain"
    ],
    "homepage": "http://contain-project.org",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Andrew Kandels",
            "email": "akandels@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/andrew-kandels/contain/issues",
        "irc": "irc://irc.freenode.net/contain"
    },
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "zendframework/zend-filter": "~2.2",
        "zendframework/zend-inputfilter": "~2.2",
        "zendframework/zend-servicemanager": "~2.2",
        "zendframework/zend-stdlib": "~2.2",
        "zendframework/zend-validator": "~2.2"
    },
    "suggest": {
        "akandels/contain-mapper": "dev-master",
        "zendframework/zend-filter": "~2.2",
        "zendframework/zend-form": "~2.2",
        "zendframework/zend-inputfilter": "~2.2",
        "zendframework/zend-mvc": "~2.2",
        "zendframework/zend-servicemanager": "~2.2",
        "zendframework/zend-validator": "~2.2",
        "symfony/yaml": ">=2.1.0"
    },
    "autoload": {
        "psr-0": {
            "Contain": "src/",
            "ContainTest": "tests/"
        }
    },
    "bin": [
        "scripts/contain-compiler.php"
    ]
}
